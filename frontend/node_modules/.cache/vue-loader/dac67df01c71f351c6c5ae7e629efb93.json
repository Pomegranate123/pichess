 const data = { 'password': this.password, 'username': this.username }\n      this.axios\n        .post('http://localhost/api/accounts/authenticate/', data, headers)\n        .then(() => {\n          this.$router.push({ name: 'home' })\n        })\n        .catch(error => {\n          console.log(error)\n          this.wrong_info()\n          this.password = ''\n          this.errormsg = 'Inloggegevens verkeerd'\n      })\n    }\n  }\n}\n</script>\n"]}]}